#!/bin/sh

if [ -z "$1" || -z "$2" ]; then
    echo ""
    echo "Usage: ${0##*/} PATH_TO_WPS_DIR Vtable_type(ex: GFS)"
    exit 0
else
    WRF_DIR=$1
    TABLE=$2
fi

ln -sf $WPS_DIR/ungrib.exe .
ln -sf $WPS_DIR/geogrid.exe .
ln -sf $WPS_DIR/metgrid.exe .
ln -sf $WPS_DIR/ungrib/Variable_Tables/Vtable.$TABLE Vtable
